												<xs:attribute name="Серия" use="required">
													<xs:annotation>
														<xs:documentation>Серия бланка свидетельства</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="Номер" use="required">
													<xs:annotation>
														<xs:documentation>Номер бланка свидетельства</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="7"/>
															<xs:maxLength value="9"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ДатаВыдСвид" use="required">
													<xs:annotation>
														<xs:documentation>Дата выдачи свидетельства </xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:date"/>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ГРНИПДатаИспрПред" type="fnst:ИдГРНИПДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата записи, в которую внесены исправления</xs:documentation>
												<xs:documentation>Формируется только в случае, если данной записью были внесены исправления (корректировки) в сведения другой записи</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ГРНИПДатаНедПред" type="fnst:ИдГРНИПДатаТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>ГРН и дата записи, которая признана недействительной</xs:documentation>
												<xs:documentation>Формируется только в случае, если данной записью  была признана недействительной (ошибочной)  другая запись</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвСтатусЗап" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о статусе записи</xs:documentation>
												<xs:documentation>Формируется только в случае, если в данную запись были внесены исправления (корректировки) другой записью или данная запись признана недействительной (ошибочной) другой записью</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ГРНИПДатаНед" type="fnst:ИдГРНИПДатаТип" minOccurs="0">
														<xs:annotation>
															<xs:documentation>ГРН и дата внесения записи, которой запись признана недействительной</xs:documentation>
															<xs:documentation>Отсутствует, если данная запись не была признана недействительной (ошибочной) другой записью </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ГРНИПДатаИспр" type="fnst:ИдГРНИПДатаТип" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>ГРН и дата записи, которой внесены исправления в связи с технической ошибкой</xs:documentation>
															<xs:documentation>Отсутствует, если в сведениях, внесенных в ЕГРИП по данной записи, не исправлялись технические ошибки </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ИдЗап" type="fnst:number-19" use="required">
										<xs:annotation>
											<xs:documentation>Системный идентификатор записи</xs:documentation>
											<xs:documentation>При наличии государственного регистрационного номера записи может совпадать с этим номером</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ГРНИП" type="fnst:OGRNSoleProprietorType" use="optional">
										<xs:annotation>
											<xs:documentation>Государственный регистрационный номер записи</xs:documentation>
											<xs:documentation>Может отсутствовать, если запись в ЕГРИП внесена без присвоения государственного регистрационного номера</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаЗап" use="required">
										<xs:annotation>
											<xs:documentation>Дата внесения записи в ЕГРИП</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date"/>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ДатаВып" type="xs:date" use="required">
							<xs:annotation>
								<xs:documentation>Дата формирования сведений из ЕГРИП в отношении индивидуального предпринимателя</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ОГРНИП" type="fnst:OGRNSoleProprietorType" use="required">
							<xs:annotation>
								<xs:documentation>Основной государственный регистрационный номер индивидуального предпринимателя</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ДатаОГРНИП" type="xs:date" use="required">
							<xs:annotation>
								<xs:documentation>Дата присвоения ОГРНИП</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ИННФЛ" type="fnst:PhysicalPersonINNType" use="optional">
							<xs:annotation>
								<xs:documentation>ИНН физического лица</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="КодВидИП" use="required">
							<xs:annotation>
								<xs:documentation>Код вида предпринимательства: индивидуальный предприниматель/глава крестьянского (фермерского) хозяйства</xs:documentation>
								<xs:documentation>, где:
												1 – индивидуальный предприниматель;
2 – глава крестьянского фермерского хозяйства</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="fnst:string-1">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="НаимВидИП" type="fnst:string-50" use="required">
							<xs:annotation>
								<xs:documentation>Наименование вида предпринимательства: индивидуальный предприниматель/глава крестьянского (фермерского) хозяйства</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="КодОбр">
					<xs:annotation>
						<xs:documentation>Код обработки</xs:documentation>
						<xs:documentation>Перечень возможных значений кода обработки приведен в Руководстве пользователя сервиса</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="fnst:string-2"/>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="ИдДок" type="fnst:string-36" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор запроса</xs:documentation>
					<xs:documentation>Указывается GUID запроса</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>
